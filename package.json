{
  "name": "lisa-box",
  "version": "0.0.22",
  "description": "Home automation system L.I.S.A.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run copy && tsc -p ./src/tsconfig.release.json",
    "build:hot": "tsc -p ./src/tsconfig.release.json",
    "build:closure": "tscc -p ./src/tsconfig.release.json",
    "lint": "tslint -p ./src",
    "watch": "tsc -w -p ./src/tsconfig.release.json",
    "pre-test": "run-s clean lint build",
    "test": "eslint --fix . && NODE_ENV=testing mocha",
    "publishPatch": "npm version patch && npm run push",
    "publishMinor": "npm version minor && npm run push",
    "publishMajor": "npm version major && npm run push",
    "push": "git push --tags && git push",
    "compile": "npm run clean && npm run build",
    "compile:hot": "npm run build:hot",
    "compile:prod": "npm run clean && npm run build:closure",
    "clean": "rm -rf dist",
    "copy": "copyfiles -u 1 src/public/*/** src/public/*/**/* dist",
    "debug": "npm run compile && ndb --prof node dist/server",
    "start": "npm run compile && node dist/server",
    "start:build": "tsc -p ./src/tsconfig.release.json && node dist/server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mylisabox/lisa-box.git"
  },
  "pre-commit": [],
  "keywords": [
    "lisa",
    "mylisabox",
    "home",
    "automation"
  ],
  "author": "jaumard",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/mylisabox/lisa-box/issues"
  },
  "homepage": "https://github.com/mylisabox/lisa-box#readme",
  "devDependencies": {
    "@fabrix/lint": "^1.0.0-alpha.3",
    "mocha": "^3.4.2",
    "pre-commit": "^1.2.2",
    "smokesignals": "^3.0.3",
    "supertest": "^3.0.0",
    "ts-node": "^8.3.0",
    "tslib": "~1.9.0",
    "tslint": "~5.14.0",
    "tslint-microsoft-contrib": "~6.1.0",
    "tsutils": "~3.9.1",
    "typescript": "~3.3.4000",
    "typescript-closure-compiler": "^1.8.11"
  },
  "dependencies": {
    "@fabrix/fabrix": "^1.6.4",
    "@fabrix/spool-errors": "^1.6.2",
    "@fabrix/spool-express": "^1.6.9",
    "@fabrix/spool-i18n": "^1.6.0",
    "@fabrix/spool-joi": "^1.6.3",
    "@fabrix/spool-realtime": "^1.6.6",
    "@fabrix/spool-repl": "^1.5.0",
    "@fabrix/spool-router": "^1.6.4",
    "@fabrix/spool-sequelize": "1.6.18",
    "bonjour": "^3.5.0",
    "cache-manager": "^2.9.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "joi": "^12.0.0",
    "jpeg-extractor": "^0.1.3",
    "lisa-discovery": "^1.0.1",
    "lisa-lirc": "^0.0.1",
    "lisa-plugins-manager": "^0.0.17",
    "lodash": "^4.17.11",
    "mdns-js": "^1.0.3",
    "mjpeg-proxy": "^0.2.0",
    "multer": "^1.4.1",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "primus-emitter": "^3.1.1",
    "primus-rooms": "^3.4.1",
    "request": "^2.88.0",
    "sqlite3": "^4.0.8",
    "trailpack-acl": "^2.0.0",
    "trailpack-bootstrap": "^2.0.0",
    "trailpack-cache": "^3.0.0",
    "trailpack-chatbot": "^2.0.8",
    "trailpack-cron": "^2.0.0",
    "trailpack-email": "^2.0.1",
    "trailpack-express": "^2.0.6",
    "trailpack-footprints": "^2.0.0",
    "trailpack-greenlock": "^0.2.2",
    "trailpack-passport": "^2.2.2",
    "trailpack-realtime": "^2.0.0",
